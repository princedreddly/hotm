<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
		<link rel="stylesheet" href="/stylesheets/css/style.css" />
		<link rel="stylesheet" href="/stylesheets/css/filtrostyle.css" />
		<link rel="stylesheet" href="/stylesheets/css/map.css" />
		<meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="HandheldFriendly" content="true">
		</head>
	<body>

		<header id="header">
			<div class="container">

				<div class="img">
					<a href="/views/map.ejs">
						<img src="/media/croppedlogo.png" alt="hola k ase" class="logo" >
					</a>
				</div>
				<div class="dropdownHeader">
					<button class="dropbtnHeader">Project</button>
					<div class="dropdown-contentHeader">
						<a href="#">Link 1</a>
						<a href="#">Link 2</a>
						<a href="#">Link 3</a>
					</div>
				</div>
				<div>
					<a href="">Repository Toolbox</a>
				</div>
				<div>
					<a href="">European Network</a>
				</div>
				<div class="dropdownHeader">
					<button class="dropbtnHeader">Collaboration</button>
					<div class="dropdown-contentHeader">
						<a href="#">Link 1</a>
						<a href="#">Link 2</a>
						<a href="#">Link 3</a>
					</div>
				</div>
				<div class="dropdownHeader">
					<button class="dropbtnHeader">Schedule</button>
					<div class="dropdown-contentHeader">
						<a href="#">Link 1</a>
						<a href="#">Link 2</a>
						<a href="#">Link 3</a>
					</div>
				</div>
				<div>
					<a href="">Contact</a>
				</div>
			</div>
		</header>

		<div id="markerInfoWindow" style="display:none";>
			<div class="topBar">
			
				<button type ="button" onclick="hideMarkerInfo()">home</button>
		

				<h2>{{data.healthCenter}}</h2>
			</div>
			<div class="leftSide">
				<div class="image"></div>
				<div class="location">{{data.location}}</div>
				<br>
				<a><div class="howToGetThere" style="border-radius: 50%; height: 50px; width: 50px; background: red"></div></a>
				<a><div class="website" style="border-radius: 50%; height: 50px; width: 50px; background: teal"></div></a>
			</div>
			<div class="rightSide">
					<h2>Information</h2>
					<p>{{data.description}}</p>
					<hr>
					<strong>Health Center:</strong>{{data.healthCenter}}
					<hr>
					<strong>Type of organization:</strong>{{data.type}}
					<hr>
					<strong>Person in charge:</strong>{{data.personInCharge}}
					<hr>
					<strong>Email:</strong>{{data.email}}
					<hr>
					<strong>Telephone contact:</strong>{{data.telephone}}
					<hr>
					<strong>Website:</strong>{{data.website}}
					<hr>
					<strong>Access requirements:</strong>{{data.accessRequirements}}
					<hr>
					<div class="freeLogo"></div>
					<strong>Free:</strong>{{data.free}}
					<div class="ConcertedLogo"></div>
					<strong>Concerted:</strong>{{data.concerted:}}
					<div class="SubsidizedLogo"></div>
					<strong>Subsidized:</strong>{{data.subsidized}}
					<div class="ChargesLogo"></div>
					<strong>Charges:</strong>{{data.charges}}
					<br>
					<br>
					<strong>Populations served</strong>
					<div class="tagsContainer">
						{{data.populationServed}}
					</div>
					<hr>
					{{data.genderObservations}}
					<br>
					<strong>Services Offered</strong>
					<div class="tagsContainer">
						{{data.}}
					</div>
					<hr>
					<strong>Spoken languages</strong>
					<div class="tagsContainer">
						{{data.spokenLanguages}}
					</div>
					<hr>
					{{data.languageObservations}}
					
			</div>
		</div>

		

		<div id="mapContainer">
			<%- include("navbar") %>
			
			<div id="map"/>
		
		</div>


		

		<script src="/javascripts/map/mainMap.js"></script>
		<script src="/javascripts/map/filters.js"></script>
		<script
			async
			defer
			src="https://maps.googleapis.com/maps/api/js?callback=initMap"></script>
	</body>
</html>
